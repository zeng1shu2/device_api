      "ResponseBase": {
        "type": "object",
        "properties": {
          "Msg": {
            "type": "string",
            "description": "成功标志",
            "nullable": true
          }
        },
        "additionalProperties": false,
        "description": " 通用返回值"
      },
      "BaseError400Response": {
        "type": "object",
        "properties": {
          "Code": {
            "type": "integer",
            "format": "int32"
          },
          "ErrorMsg": {
            "type": "string",
            "description": "错误提示",
            "nullable": true
          }
        },
        "additionalProperties": false,
        "description": "根据不同的请求参数返回不同的错误代码"
      },
      "UserRequest": {
        "type": "object",
        "properties": {
          "VPN账号权限": {
            "type": "integer",
            "example": [
              "0"
            ],
            "description": "0 表示未勾选 1 表示勾选"
          },
          "VPN权限类型": {
            "type": "string",
            "example": [
              "type"
            ]
          },
          "公司名称": {
            "type": "string",
            "example": [
              "dept"
            ],
            "description": "致远：7893214819773270159 立功科技：-8486162257746376183"
          },
          "计算机名": {
            "type": "string",
            "example": [
              "name"
            ]
          },
          "申请人": {
            "type": "string",
            "example": [
              "sname"
            ]
          },
          "VPN用户组": {
            "type": "string",
            "example": [
              "group"
            ]
          },
          "VPN_YF": {
            "type": "string",
            "example": [
              "if"
            ],
            "description": "0：表示研发组 1：表示职能组"
          },
          "结束时间": {
            "type": "string",
            "example": [
              "time"
            ]
          }
        },
        "additionalProperties": false
      },
      "UserModifRequest": {
        "type": "object",
        "properties": {
